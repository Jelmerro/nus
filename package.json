{
    "name": "nus",
    "version": "0.1.0",
    "description": "A script to update all node packages in a project",
    "keywords": [
        "Nodejs",
        "Npm",
        "Updater"
    ],
    "homepage": "https://github.com/Jelmerro/nus",
    "bugs": "https://github.com/Jelmerro/nus/issues/",
    "repository": {
        "url": "https://github.com/Jelmerro/nus",
        "type": "git"
    },
    "funding": "https://github.com/sponsors/Jelmerro/",
    "license": "MIT",
    "author": "Jelmer van Arnhem",
    "type": "module",
    "exports": "./app/index.js",
    "bin": {
        "nus": "./app/index.js"
    },
    "scripts": {
        "eslint": "eslint",
        "fix": "eslint --fix",
        "lint": "npm run eslint && npm run tsc",
        "tsc": "tsc",
        "update": "npx -y jelmerro/nus"
    },
    "dependencies": {
        "fast-npm-meta": "1.2.0",
        "semver": "7.7.4"
    },
    "devDependencies": {
        "@types/semver": "7.7.1",
        "eslint-config": "github:Jelmerro/eslint-config",
        "type-config": "github:Jelmerro/type-config"
    },
    "engines": {
        "node": ">=16"
    }
}
